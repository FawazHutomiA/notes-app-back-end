{
    "env": {
        "es2021": true,
        "node": true
    },
    "extends": [
        "airbnb-base"
    ],
    "parserOptions": {
        "ecmaVersion": 2020
    },
    "rules": {
      "no-console": "off",
      "no-underscore-dangle": "off",
      "eol-last": 0,
      "no-multiple-empty-lines": ["error", { "max": 1, "maxEOF": 0 }],
      "quotes": "off"
    }
}
